   <thing-type id="3306">
      <supported-bridge-type-refs>
         <bridge-type-ref id="lwm2mBridgeThing"/>
      </supported-bridge-type-refs>
      <label>Actuation</label>
      <description><![CDATA[This IPSO object is dedicated to remote actuation such as ON/OFF action or dimming. A multi-state output can also be described as a string. This is useful to send pilot wire orders for instance. It also provides a resource to reflect the time that the device has been switched on.]]></description>
      <!--Optional channel Dimmer (5851) added dynamically-->
      <!--Optional channel On Time (5852) added dynamically-->
      <!--Optional channel Muti-state Output (5853) added dynamically-->
      <!--Optional channel Application Type (5750) added dynamically-->
      <channels>
         <channel id="5850" typeId="5850"/>
         <!--Channel On/Off (5850)-->
      </channels>
      <properties>
         <property name="ObjectID">3306</property>
         <property name="ObjectURN">urn:oma:lwm2m:ext:3306</property>
         <property name="MultipleInstances">Multiple</property>
         <property name="Mandatory">Optional</property>
      </properties>
   </thing-type>
</thing:thing-descriptions>
