   <thing-type id="3304">
      <supported-bridge-type-refs>
         <bridge-type-ref id="lwm2mBridgeThing"/>
      </supported-bridge-type-refs>
      <label>Humidity</label>
      <description><![CDATA[Description: This IPSO object should be used with a humidity sensor to report a humidity measurement.  It also provides resources for minimum/maximum measured values and the minimum/maximum range that can be measured by the humidity sensor. An example measurement unit is relative humidity as a percentage (ucum:%).]]></description>
      <!--Optional channel Min Measured Value (5601) added dynamically-->
      <!--Optional channel Max Measured Value (5602) added dynamically-->
      <!--Optional channel Min Range Value (5603) added dynamically-->
      <!--Optional channel Max Range Value (5604) added dynamically-->
      <!--Optional channel Sensor Units (5701) added dynamically-->
      <!--Optional channel Reset Min and Max Measured Values (5605) added dynamically-->
      <channels>
         <channel id="5700" typeId="5700"/>
         <!--Channel Sensor Value (5700)-->
      </channels>
      <properties>
         <property name="ObjectID">3304</property>
         <property name="ObjectURN">urn:oma:lwm2m:ext:3304</property>
         <property name="MultipleInstances">Multiple</property>
         <property name="Mandatory">Optional</property>
      </properties>
   </thing-type>
